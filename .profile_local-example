# This file is for profile things SPECIFIC to THIS computer.



# Set the ORACLE_BASE
export ORACLE_BASE=/u01/app/oracle

# Set the ORACLE_HOME.  Modify this depending on which DB you want to access.
export ORACLE_HOME=${ORACLE_BASE}/product/12.2.0.1/dbhome_200114_1

# Add the ORACLE_HOME binaries to the PATH
export PATH=$PATH:${ORACLE_HOME}/bin



# Python virtualenvwrapper (see https://virtualenvwrapper.readthedocs.io/en/latest/)
# source /usr/local/bin/virtualenvwrapper.sh # stopped working, probably from reinstalling python or something
export WORKON_HOME="${HOME}/.virtualenvs"
. /usr/local/bin/virtualenvwrapper_lazy.sh

# put Python bin executables in PATH
PATH="/Users/Matthew/Library/Python/3.7/bin:${PATH}"
PATH="/Library/Frameworks/Python.framework/Versions/2.7/bin:${PATH}"



# Flutter path
PATH="${PATH}:/usr/local/lib/flutter/bin"



# Java path (not installed, but example here)
# JAVA_HOME=/Library/Java/JavaVirtualMachines/jdk1.8.0_191.jdk/Contents/Home



# export any changes made
export PATH


